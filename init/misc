#!/usr/bin/env bash

# iStat Menus 7
curl -sSo $HOME/Downloads/iStatMenu7.zip -L https://download.bjango.com/istatmenus7/

# Source Han Mono
curl -sSo $HOME/Downloads/SourceHanMono.ttc -L https://github.com/adobe-fonts/source-han-mono/releases/download/1.002/SourceHanMono.ttc

# UDEV Gothic NF
curl -sSo $HOME/Downloads/UDEVGothic_NF.zip -L https://github.com/yuru7/udev-gothic/releases/download/v2.1.0/UDEVGothic_NF_v2.1.0.zip
